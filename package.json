{
  "name": "hawthorn-client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,json,jsx,less}'",
