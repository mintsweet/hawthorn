    "@umijs/test": "^3.0.16",
    "antd": "^4.2.0",
    "mints-utils": "^1.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.0.16"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5"
  }
}
